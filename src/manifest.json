{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "default_locale": "en_US",
  "version": "25.0.0",
  "author": "Firefox",
  "applications": {
    "gecko": {
      "id": "secure-proxy@mozilla.com",
      "strict_min_version": "71.0",
      "update_url": "https://fpn.firefox.com/downloads/browser/updates.json"
    }
  },

  "homepage_url": "https://fpn.firefox.com",

  "description": "__MSG_extensionDescription__",
  "browser_action": {
    "browser_style": true,
    "default_icon": "img/badge_warning.svg",
    "default_title": "__MSG_extensionName__",
    "default_popup": "popup/popup.html"
  },

  "icons": {
    "48": "img/icon.svg",
    "96": "img/icon.svg"
  },

  "incognito": "spanning",

  "permissions": [
      "<all_urls>",
      "browserSettings",
      "captivePortal",
      "identity",
      "idle",
      "management",
      "privacy",
      "proxy",
      "storage",
      "tabs",
      "telemetry",
      "webRequest",
      "webRequestBlocking"
  ],

  "experiment_apis": {
    "proxyutils": {
      "schema": "experiments/proxyutils/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "experiments/proxyutils/api.js",
        "paths": [["experiments", "proxyutils"]]
      }
    }
  },

  "web_accessible_resources": ["fonts/*.woff2"],

  "background": {
    "page": "background/page.html"
  }
}
