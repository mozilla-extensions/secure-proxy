[
  {
    "namespace": "experiments.proxyutils",
    "description": "Firefox Proxy internal API for accessing Firefox stuff.",
    "types": [
    ],
    "events": [
      {
        "name": "onChanged",
        "type": "function",
        "description": "Fired when the proxy setting is changed.",
        "parameters": []
      }
    ],
    "functions": [
      {
        "name": "showPrompt",
        "type": "function",
        "description": "show the prompt.",
        "async": true,
        "parameters": [
          {
            "name": "message",
            "type": "string",
            "description": "text to display in the prompt"
          },
          {
            "name": "isWarning",
            "type": "boolean",
            "description": "if the prompt should be a warning"
          }
        ]
      },
      {
        "name": "getCaptivePortalURL",
        "type": "function",
        "async": true,
        "description": "Get the captive portal URL.",
        "parameters": []
      },
      {
        "name": "getDebuggingMode",
        "type": "function",
        "async": true,
        "description": "Returns true if we are in debugging mode. It reads the pref secureProxy.debugging.enabled",
        "parameters": []
      },
      {
        "name": "formatURL",
        "type": "function",
        "async": true,
        "description": "Returns a formatted URL",
        "parameters": [
          {
            "name": "url",
            "type": "string",
            "description": "the URL to format"
          }
        ]
      },
      {
        "name": "loadNetError",
        "type": "function",
        "async": true,
        "description": "Loads a net error",
        "parameters": [
          {
            "name": "errorCode",
            "type": "integer",
            "description": "the neterror to load"
          },
          {
            "name": "url",
            "type": "string",
            "description": "the url being loaded"
          },
          {
            "name": "tabId",
            "type": "integer",
            "description": "the tabId to load it into"
          }
        ]
      }
    ],
    "properties": {
      "DNSoverHTTPEnabled": {
        "$ref": "types.Setting",
        "description": "This property controls DOH"
      }
    }
  }
]
